G._ = 'local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

-- GUI Principal
local gui = Instance.new("ScreenGui")
gui.Name = "SettingsGui"
gui.Parent = player:WaitForChild("PlayerGui")
gui.ResetOnSpawn = false
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
gui.DisplayOrder = 999999

-- Botão de engrenagem
local gearButton = Instance.new("ImageButton")
gearButton.Name = "GearButton"
gearButton.Parent = gui
gearButton.Size = UDim2.new(0, 60, 0, 60)
gearButton.Position = UDim2.new(0, 60, 0, 150)
gearButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
gearButton.BackgroundTransparency = 0.4
gearButton.Image = "rbxassetid://6031075938"
gearButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
gearButton.BorderSizePixel = 0
gearButton.AnchorPoint = Vector2.new(0.5, 0.5)
gearButton.ClipsDescendants = true
gearButton.ZIndex = 999

local uicorner = Instance.new("UICorner", gearButton)
uicorner.CornerRadius = UDim.new(1, 0)

local stroke = Instance.new("UIStroke", gearButton)
stroke.Thickness = 2
stroke.Color = Color3.fromRGB(100, 200, 255)

-- Movimento do botão (Mobile)
local draggingBtn, dragStartBtn, startPosBtn
gearButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Touch then
        draggingBtn = true
        dragStartBtn = input.Position
        startPosBtn = gearButton.Position
    end
end)

gearButton.InputChanged:Connect(function(input)
    if draggingBtn and input.UserInputType == Enum.UserInputType.Touch then
        local delta = input.Position - dragStartBtn
        gearButton.Position = startPosBtn + UDim2.new(0, delta.X, 0, delta.Y)
    end
end)

gearButton.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Touch then
        draggingBtn = false
    end
end)

-- Painel principal
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 600, 0, 450)
mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
mainFrame.BackgroundTransparency = 0.4
mainFrame.Visible = false
mainFrame.ZIndex = 999
mainFrame.Parent = gui

local corner = Instance.new("UICorner", mainFrame)
corner.CornerRadius = UDim.new(0.05, 0)

local mainStroke = Instance.new("UIStroke", mainFrame)
mainStroke.Color = Color3.fromRGB(100, 200, 255)
mainStroke.Thickness = 2

-- Título e subtítulo
