-- AimLock Functional Script
local aimEnabled = false

local function isVisible(target)
    local origin = game.Players.LocalPlayer.Character.Head.Position
    local direction = (target.Position - origin)
    local ray = Ray.new(origin, direction)
    local part, hit = workspace:FindPartOnRay(ray, game.Players.LocalPlayer.Character, false, true)
    return hit == target or (part and part:IsDescendantOf(target.Parent))
end

local function getClosestEnemy()
    local closest, distance = nil, math.huge
    for _, plr in pairs(game.Players:GetPlayers()) do
        if plr ~= game.Players.LocalPlayer and plr.Team ~= game.Players.LocalPlayer.Team then
            local char = plr.Character
            if char and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChild("Head") then
                if isVisible(char.Head) then
                    local magnitude = (char.Head.Position - game.Workspace.CurrentCamera.CFrame.Position).Magnitude
                    if magnitude < distance then
                        distance = magnitude
                        closest = char.Head
                    end
                end
            end
        end
    end
    return closest
end

game:GetService("RunService").RenderStepped:Connect(function()
    if aimEnabled then
        local closest = getClosestEnemy()
        if closest then
            local camera = game.Workspace.CurrentCamera
            camera.CFrame = CFrame.new(camera.CFrame.Position, closest.Position)
        end
    end
end)

-- Toggle Logic
aimToggle.MouseButton1Click:Connect(function()
    aimEnabled = not aimEnabled
    if aimEnabled then
        aimToggle.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        knob:TweenPosition(UDim2.new(0.5, 2, 0, 2), "Out", "Sine", 0.2)
    else
        aimToggle.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        knob:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Sine", 0.2)
    end
end)
