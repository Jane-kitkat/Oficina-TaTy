-- ESP Logic
local espEnabled = false
local boxes = {}

local function createESPBox(player)
    local box = Drawing.new("Square")
    box.Visible = false
    box.Color = Color3.fromRGB(0, 255, 255)
    box.Thickness = 1
    box.Transparency = 1
    box.Filled = false
    boxes[player] = box
end

local function removeESPBox(player)
    if boxes[player] then
        boxes[player]:Remove()
        boxes[player] = nil
    end
end

for _, plr in pairs(game.Players:GetPlayers()) do
    if plr ~= game.Players.LocalPlayer and plr.Team ~= game.Players.LocalPlayer.Team then
        createESPBox(plr)
    end
end

game.Players.PlayerAdded:Connect(function(plr)
    if plr ~= game.Players.LocalPlayer then
        plr.CharacterAdded:Connect(function()
            wait(1)
            if plr.Team ~= game.Players.LocalPlayer.Team then
                createESPBox(plr)
            end
        end)
    end
end)

game.Players.PlayerRemoving:Connect(function(plr)
    removeESPBox(plr)
end)

game:GetService("RunService").RenderStepped:Connect(function()
    for player, box in pairs(boxes) do
        local character = player.Character
        local hrp = character and character:FindFirstChild("HumanoidRootPart")
        if character and hrp and espEnabled then
            local pos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(hrp.Position)
            local head = character:FindFirstChild("Head")
            local torso = character:FindFirstChild("UpperTorso") or character:FindFirstChild("Torso")

            if onScreen and head and torso then
                local headPos = workspace.CurrentCamera:WorldToViewportPoint(head.Position + Vector3.new(0, 0.5, 0))
                local torsoPos = workspace.CurrentCamera:WorldToViewportPoint(torso.Position - Vector3.new(0, 0.5, 0))
                box.Size = Vector2.new(math.abs(headPos.X - torsoPos.X) * 2, math.abs(torsoPos.Y - headPos.Y) * 2)
                box.Position = Vector2.new(pos.X - box.Size.X / 2, headPos.Y)
                box.Visible = true
            else
                box.Visible = false
            end
        else
            box.Visible = false
        end
    end
end)

-- Toggle Logic
espToggle.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    if espEnabled then
        espToggle.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        espKnob:TweenPosition(UDim2.new(0.5, 2, 0, 2), "Out", "Sine", 0.2)
    else
        espToggle.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        espKnob:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Sine", 0.2)
    end
