local switchKnob = Instance.new("Frame")
switchKnob.Size = UDim2.new(0.5, 0, 1, 0)
switchKnob.Position = UDim2.new(0, 0, 0, 0)
switchKnob.BackgroundColor3 = Color3.new(1, 1, 1)  
switchKnob.BorderSizePixel = 0
switchKnob.Parent = switchFrame

local uiCornerKnob = Instance.new("UICorner")
uiCornerKnob.CornerRadius = UDim.new(0, 12)
uiCornerKnob.Parent = switchKnob

local killAuraEnabled = false


local function toggleKillAura()
    killAuraEnabled = not killAuraEnabled
    if killAuraEnabled then
        TweenService:Create(switchKnob, TweenInfo.new(0.2), {Position = UDim2.new(0.5, 0, 0, 0)}):Play()
        switchFrame.BackgroundColor3 = Color3.new(0, 1, 0)  
    else
        TweenService:Create(switchKnob, TweenInfo.new(0.2), {Position = UDim2.new(0, 0, 0, 0)}):Play()
        switchFrame.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)  
    end
end

switchFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        toggleKillAura()
    end
end)


