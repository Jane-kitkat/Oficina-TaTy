local npcsFolder = workspace:WaitForChild("NPCs")

local function killAura()
    if not killAuraEnabled then return end
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    local root = character.HumanoidRootPart
    local position = root.Position
    
    
    local npcs = npcsFolder:GetChildren()
    
    
    local taggedNpcs = CollectionService:GetTagged("Normal")
    for _, tagged in ipairs(taggedNpcs) do
        if not table.find(npcs, tagged) then
            table.insert(npcs, tagged)
        end
    end
    
    for _, npc in ipairs(npcs) do
        if npc:IsA("Model") and npc.Name == "Normal" then
            local humanoid = npc:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.Health > 0 then
                local npcRoot = npc:FindFirstChild("HumanoidRootPart")
                if npcRoot and (position - npcRoot.Position).Magnitude <= 100 then
                    humanoid:TakeDamage(humanoid.Health)  
                end
            end
        end
    end
end


local debris = workspace:FindFirstChild("Debris")  

local function autoIfood()
    if not autoIfoodEnabled then return end
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    local root = character.HumanoidRootPart
    
    
    local deliveryGuy = workspace:FindFirstChild("DeliveryGuy")
    local npc = if debris then debris:FindFirstChild("NPC") else nil
    
    local target = nil
    if deliveryGuy and deliveryGuy:IsA("Model") then
        target = deliveryGuy
    elseif npc and npc:IsA("Model") then
        target = npc
    end
    
    if not target then
        
        autoIfoodEnabled = false
        TweenService:Create(switchKnobAutoIfood, TweenInfo.new(0.2), {Position = UDim2.new(0, 0, 0, 0)}):Play()
        switchFrameAutoIfood.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)
        return
    end
    
    local targetRoot = target:FindFirstChild("HumanoidRootPart")
    if targetRoot then
        
        root.CFrame = targetRoot.CFrame * CFrame.new(5, 0, 0)
    end
end


local function autoHelp()
    if not autoHelpEnabled then return end
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    local root = character.HumanoidRootPart
    
    
    local hospitalDeliveryGuy = workspace:FindFirstChild("HospitalDeliveryGuy")
    local patient = if debris then debris:FindFirstChild("Patient") else nil
    
    local target = nil
    if hospitalDeliveryGuy and hospitalDeliveryGuy:IsA("Model") then
        target = hospitalDeliveryGuy
    elseif patient and patient:IsA("Model") then
        target = patient
    end
    
    if not target then
        
        autoHelpEnabled = false
        TweenService:Create(switchKnobAutoHelp, TweenInfo.new(0.2), {Position = UDim2.new(0, 0, 0, 0)}):Play()
        switchFrameAutoHelp.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)
        return
    end
    
    local targetRoot = target:FindFirstChild("HumanoidRootPart")
    if targetRoot then
        
        root.CFrame = targetRoot.CFrame * CFrame.new(5, 0, 0)
    end
end


RunService.Heartbeat:Connect(killAura)
RunService.Heartbeat:Connect(autoIfood)
RunService.Heartbeat:Connect(autoHelp)


button.MouseButton1Click:Connect(function()
    panel.Visible = not panel.Visible
end)
